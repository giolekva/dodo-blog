{
	"$schema": "https://d.p.v1.dodo.cloud/schemas/gio/app.schema.json",
	"app": {
		"dev": {
			"enabled": true,
			"username": "gio"
		},
		"ingress": {
			"auth": {
				"enabled": true,
				"noAuthPathPatterns": [
					"^/_next/.*$",
					"^/$",
					"^/entry/.*$"
				]
			},
			"network": "Private",
			"subdomain": "dev-blog"
		},
		"type": "nextjs:deno-2.0.0",
		"postgresql": [{
			"name": "db",
			"size": "1Gi"
		}]
	}
}